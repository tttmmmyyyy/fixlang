module Test;

import NormalDep;
import TestDep;

test: IO ();
test = (
    // Test using both normal-dep and test-dep
    let greeting = NormalDep::greet("Test");
    let expected = "Hello from normal-dep, Test!";
    let result = if greeting == expected { "PASS" } else { "FAIL" };
    let msg = TestDep::assert_msg("test_greet", result);
    println(msg)
);
