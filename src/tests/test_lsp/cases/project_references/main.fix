module Main;

import Lib;

// A function that uses helper from Lib.
use_helper : I64 -> I64;
use_helper = |x| Lib::helper(x) + Lib::helper(x + 1);

// A function that uses MyData type.
make_data : I64 -> Lib::MyData;
make_data = |v| Lib::MyData { value: v };

// A function that calls use_helper.
caller : I64 -> I64;
caller = |x| use_helper(x) + use_helper(x + 1);

main : IO ();
main = (
    let result = caller(0);
    let _data = make_data(result);
    pure()
);
