module Lib;

// A type used for testing type references.
type MyData = unbox struct {
    value: I64
};

// A helper function called from multiple places.
helper : I64 -> I64;
helper = |x| x + 1;

// Another helper that calls the first one.
double_helper : I64 -> I64;
double_helper = |x| helper(helper(x));
