trait a : MyEq {
    my_eq : a -> a -> Bool;
}

type Pair a b = unbox struct { fst: a, snd: b };

impl [a : MyEq, b : MyEq] Pair a b : MyEq {
    my_eq = |lhs, rhs| lhs.@fst.my_eq(rhs.@fst) && lhs.@snd.my_eq(rhs.@snd);
}

